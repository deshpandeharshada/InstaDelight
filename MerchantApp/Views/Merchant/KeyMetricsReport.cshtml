@{
    ViewBag.Title = "KeyMetricsReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link rel="stylesheet" href="~/Content/fonts/opensans/css/fonts.css">
<link rel="stylesheet" href="~/Content/css/foundation.css">

<link rel="stylesheet" href="~/Content/css/font-awesome.min.css">

<div class="rowb" ng-controller="myCntrl" ng-app="myApp">
    <div class="row">
        <div class="col-xs-12">
            <h1>@ViewBag.Result</h1>
        </div>
        <hr />
        <label>
            <b> @Global.Merchant.ConsumetCount: &nbsp;</b>
            @ViewBag.ConsumerCount
        </label>

        <label>
            <b>@Global.Merchant.SMSSent: &nbsp;</b>
            @ViewBag.SMSCount
        </label>

        <label>
            <b>@Global.Merchant.SMSBalance: &nbsp;</b>
            @ViewBag.NoOfSMS
        </label>

        <label>
            <b>@Global.Merchant.EmailSent: &nbsp;</b>
            @ViewBag.EmailCount
        </label>

    </div>
</div>
